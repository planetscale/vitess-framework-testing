cabal-version:       2.4
-- Initial package description 'test.cabal' generated by 'cabal init'.  For
--  further documentation, see http://haskell.org/cabal/users-guide/

name:                test
version:             0.1.0.0
build-type:          Simple
-- synopsis:
-- description:
homepage:            https://github.com/planetscale/vitess-framework-testing
-- bug-reports:
license:             NONE
author:              Mike Cronce
maintainer:          mc@planetscale.com
-- copyright:
category:            Integration test

executable test
	main-is:             Main.hs
	other-modules:       BinaryRow,
	                     BinaryRowNew,
	                     BinLog,
	                     BinLogNew,
	                     TextRow,
	                     TextRowNew,
	                     ExecuteMany
	-- other-extensions:
	build-depends:       base ^>=4.12.0.0,
	                     text >= 1.2,
	                     time >= 1.8,
	                     io-streams >= 1.5,
	                     bytestring >= 0.10,
	                     vector >= 0.12,
	                     tasty >= 0.11 && < 2.0,
	                     tasty-hunit >= 0.10,
	                     network >= 3,
	                     mysql-haskell >= 0.8
	-- hs-source-dirs:
	default-language:    Haskell2010
	default-extensions:  MultiWayIf
	                     OverloadedStrings
	                     ImplicitParams
	ghc-options:         -Wno-tabs -threaded

